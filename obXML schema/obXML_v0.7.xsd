													<xs:element name="HVAC" minOccurs="0">
														<xs:complexType>
															<xs:choice>
																<xs:element name="HVACType" type="HVACType"/>
															</xs:choice>
														</xs:complexType>
													</xs:element>
												</xs:choice>
