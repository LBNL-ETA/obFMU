																								</xs:simpleContent>
																							</xs:complexType>
																						</xs:element>
																					</xs:all>
																				</xs:complexType>
																			</xs:element>
																		</xs:choice>
																	</xs:complexType>
																</xs:element>
																<xs:element name="Events">
																	<xs:complexType>
																		<xs:choice maxOccurs="unbounded">
