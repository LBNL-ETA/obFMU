																				<xs:complexType>
																					<xs:all>
																						<xs:element name="Type" type="MovementEventType"/>
																						<xs:element name="ProbabilityModel" type="ProbabilityModelType" default="UniformDistribution"/>
																						<xs:element name="StartTime" type="xs:time"/>
																						<xs:element name="EndTime" type="xs:time"/>
																						<xs:element name="TypicalTime" type="xs:time" minOccurs="0"/>
